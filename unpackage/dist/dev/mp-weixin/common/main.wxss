














/*每个页面公共css */
page,page{
	width: 100%;
	height: 100%;
	margin: 0;
	padding:0;
}
page{margin:0;padding:0;font-size:14px;line-height:22px;  font-family:"\5FAE\8F6F\96C5\9ED1"; color:#3e3938;-webkit-text-size-adjust:none;}
page, ._div, ._p, ._h1, ._h2, ._h3, ._h4, ._h5, ._h6, ._ul, ._ol, ._li, ._dl, ._dd, ._dt, ._img, form { padding:0px; margin:0px; border:0; font-size:14px;
}
.clear{clear:both;}
._a:link, ._a:visited { text-decoration:none;
}
._a:hover { text-decoration:none;}
._a:focus { outline:none;
}
._a{text-decoration:none; blr:expression(this.onFocus=this.blur());}
._a{outline:none;}
._ul, ._ol ._li { list-style:none;
}

