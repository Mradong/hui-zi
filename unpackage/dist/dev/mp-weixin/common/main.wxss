














/*每个页面公共css */
page,page{
	width: 100%;
	height: 100%;
	margin: 0;
	padding:0;
}
page{margin:0;padding:0;font-size:14px;line-height:22px;  font-family:"\5FAE\8F6F\96C5\9ED1"; color:#3e3938;-webkit-text-size-adjust:none;}
page, ._div, ._p, ._h1, ._h2, ._h3, ._h4, ._h5, ._h6, ._ul, ._ol, ._li, ._dl, ._dd, ._dt, ._img, form { padding:0px; margin:0px; border:0; font-size:14px;
}
.clear{clear:both;}
._a:link, ._a:visited { text-decoration:none;
}
._a:hover { text-decoration:none;}
._a:focus { outline:none;
}
._a{text-decoration:none; blr:expression(this.onFocus=this.blur());}
._a{outline:none;}
._ul, ._ol ._li { list-style:none;
}
.fr{float: right;}
.fl{float: left;}
/* 字体主色调 */
.dominant-hue{
	color: #FEDD12;
}
/* 背景主色调 */
.dominant-hue-bg{
	background-color: #FEDD12 !important;
}
/* 背景渐变主色调 */
.dominant-hue-bg-shade{
	background: -webkit-linear-gradient(bottom left, #FEDD12, #f38489);
	background: linear-gradient(to top right, #FEDD12, #f38489);
}
/* 按钮红色主色调 */
.dominant-hue-button-bg-red{
	color: #FBF0DC !important;
	background-color: #FD5234 !important;
}
/* 按钮黄色主色调 */
.dominant-hue-button-bg-yell{
	color:#250D00;
	background-color: #FEDD12 !important;
}
/* 按钮渐变主色调 */
.dominant-hue-button-bg-shade{
	background: -webkit-linear-gradient(bottom left, #f38489, #ff525a);
	background: linear-gradient(to top right, #f38489, #ff525a);
}
/* 按钮按住状态主色调 */
.button-hover{color: #bbb19e !important;}

