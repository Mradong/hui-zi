
 .header_fixed {
  z-index: 99999;
}
 .uni-countdown {
  float: left;
}
.dream-banner {
  width: 100%;
  height: 400rpx;
  position: relative;
}
.dream-banner-title {
  position: absolute;
  top: 50rpx;
  z-index: 9;
  color: #ffffff;
  width: 100%;
  text-align: center;
  font-size: 40rpx;
  line-height: 64rpx;
  text-shadow: 4rpx 4rpx 10rpx #000000;
}
.dream-banner-title > text {
  font-size: 36rpx;
  position: relative;
  z-index: 99;
}
.dream-banner-title .dream-banner-pig {
  position: absolute;
  left: 50%;
  font-weight: 700;
  -webkit-transform: translate(-50%);
          transform: translate(-50%);
  z-index: 9;
}
.dream-banner-title .dream-banner-pig text {
  line-height: 100rpx;
  font-size: 68rpx;
}
.dream-banner > image {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
}
.dream-banner .icon-lyd-r,
.dream-banner .icon-lyd-l {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 20rpx;
  z-index: 99;
  opacity: 0.5;
  filter: alpha(opacity=50);
  /* 针对 IE8 以及更早的版本 */
}
.dream-banner .icon-lyd-r image,
.dream-banner .icon-lyd-l image {
  width: 100%;
  height: 100%;
}
.dream-banner .icon-lyd-r {
  right: 20rpx;
}
.dream-banner .icon-lyd-l {
  left: 20rpx;
}
.dream-detail {
  width: 90%;
  height: 150rpx;
  margin: 0 auto;
  position: relative;
  top: -40rpx;
  background-color: #ffffff;
  box-shadow: 0 0 15px #d2cfcf;
  border-radius: 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.dream-detail-item {
  padding-top: 20rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
  line-height: 50rpx;
  font-weight: 700;
  font-size: 32rpx;
}
.dream-detail-item text {
  font-weight: 500;
  color: #9a9a9a;
  font-size: 24rpx;
}
.dream-items {
  margin: 20rpx;
  margin-bottom: 140rpx;
}
.dream-items-quesheng {
  width: 90%;
  height: 400rpx;
}
.dream-items-quesheng image {
  width: 360rpx;
  height: 360rpx;
  display: block;
  margin: 0 auto;
}
.dream-items-quesheng text {
  font-size: 36rpx;
  display: block;
  text-align: center;
}
.dream-items-title {
  height: 100rpx;
}
.dream-items-title text {
  float: left;
  font-weight: bold;
  margin-left: 10rpx;
  width: 305rpx;
}
.dream-items-title image {
  width: 100rpx;
  height: 100rpx;
  float: left;
}
.dream-items .in-competition .today-item .custom-area {
  width: 95%;
  margin: 0 auto;
  color: #505050;
  font-size: 14px;
  line-height: 150%;
  text-align: center;
}
.dream-items .in-competition .today-item .custom-area-item {
  margin-top: 20rpx;
  width: 100%;
  box-shadow: 10rpx 10rpx 20rpx #c0c0c0;
}
.dream-items .in-competition .today-item .custom-area .tc,
.dream-items .in-competition .today-item .custom-area .td,
.dream-items .in-competition .today-item .custom-area .tf {
  width: 100%;
  height: 80rpx;
  color: #a6a6a6;
  font-size: 26rpx;
  line-height: 60rpx;
  border: 2rpx solid #f2f2f2;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.dream-items .in-competition .today-item .custom-area .tc-r,
.dream-items .in-competition .today-item .custom-area .td-r,
.dream-items .in-competition .today-item .custom-area .tf-r {
  padding-top: 10rpx;
  -webkit-box-flex: 2;
  -webkit-flex: 2;
          flex: 2;
  text-align: left;
}
.dream-items .in-competition .today-item .custom-area .tc-r .countdown-text,
.dream-items .in-competition .today-item .custom-area .td-r .countdown-text,
.dream-items .in-competition .today-item .custom-area .tf-r .countdown-text {
  float: left;
}
.dream-items .in-competition .today-item .custom-area .tc-r .countdown-text text,
.dream-items .in-competition .today-item .custom-area .td-r .countdown-text text,
.dream-items .in-competition .today-item .custom-area .tf-r .countdown-text text {
  padding: 8rpx;
  color: #DD524D;
  font-size: 28rpx;
}
.dream-items .in-competition .today-item .custom-area .tc-l,
.dream-items .in-competition .today-item .custom-area .td-l,
.dream-items .in-competition .today-item .custom-area .tf-l {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: right;
}
.dream-items .in-competition .today-item .custom-area .td {
  height: 210rpx;
}
.dream-items .in-competition .today-item .custom-area .td-r {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: left;
  color: #333333;
  position: relative;
}
.dream-items .in-competition .today-item .custom-area .td-r .unclosed-competition {
  margin-top: 30rpx;
  font-size: 24rpx;
  color: #ff1800;
  line-height: 36rpx;
}
.dream-items .in-competition .today-item .custom-area .td-r .shouyi {
  margin-top: 30rpx;
  font-size: 70rpx;
  line-height: 76rpx;
  color: #ff0900;
}
.dream-items .in-competition .today-item .custom-area .td-r::after {
  content: '';
  display: block;
  height: 184rpx;
  width: 2rpx;
  background-color: #f1e4e4;
  position: absolute;
  top: 50%;
  right: 0;
  -webkit-transform: translate(0, -50%);
          transform: translate(0, -50%);
}
.dream-items .in-competition .today-item .custom-area .td-l {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: left;
  margin-left: 50rpx;
  font-size: 24rpx;
}
.dream-items .in-competition .today-item .custom-area .td-l .td-l-tile {
  font-weight: 600;
  margin-top: 30rpx;
  font-size: 40rpx;
  color: #333333;
}
.dream-items .in-competition .today-item .custom-area .tf {
  height: 80rpx;
}
.dream-items .in-competition .today-item .custom-area .tf-r {
  -webkit-box-flex: 2;
  -webkit-flex: 2;
          flex: 2;
  text-align: left;
}
.dream-items .in-competition .today-item .custom-area .tf-l {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: right;
}
.dream-items .in-competition .today-item .custom-area .tf-l .tf-button {
  margin-top: 12rpx;
  width: 200rpx;
  height: 56rpx;
  color: #fff;
  box-shadow: #90c0ef solid 1px;
  border-radius: 4px;
  font-size: 26rpx;
  line-height: 56rpx;
  text-align: center;
}
.dream-tab {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 120rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.dream-tab-y {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.dream-tab-y .button-y {
  color: #ff525a;
}
.dream-tab-e {
  -webkit-box-flex: 2;
  -webkit-flex: 2;
          flex: 2;
}
.dream-tab-e .button-e {
  color: #ffffff;
}
.dream-tab button,
.dream-tab button::after {
  border-radius: 0;
  line-height: 120rpx;
}

