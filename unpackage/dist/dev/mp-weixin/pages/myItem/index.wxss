page,
page,
uni-page-body {
  width: 100%;
  height: 100%;
}
.my-item {
  width: 100%;
  height: 100%;
  background-color: #F0EFF5;
}
.my-item .item-chunk {
  height: 400rpx;
  background-color: #4563C2;
  border-radius: 6rpx;
  box-shadow: 0 2px 2px #d1d1d1;
}
.my-item .item-card {
  height: 4rpx;
  background-color: #FFFFFF;
}
.my-item .items .item {
  margin-top: 30rpx;
  background-color: #FFFFFF;
}
.my-item .items .item-title {
  height: 90rpx;
  line-height: 90rpx;
  margin-left: 20rpx;
  border-bottom: 1px solid #F0EFF5;
  font-size: 40rpx;
  color: #272931;
}
.my-item .items .item-detail {
  height: 140rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 26rpx;
  color: #969696;
}
.my-item .items .item-detail-money {
  -webkit-box-flex: 3;
  -webkit-flex: 3;
          flex: 3;
  text-align: left;
}
.my-item .items .item-detail-money-b {
  margin-top: 20rpx;
  font-size: 40rpx;
  color: #272931;
}
.my-item .items .item-detail-go {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  line-height: 140rpx;
}
.my-item .items .item-detail-go image {
  width: 40rpx;
  height: 40rpx;
}

