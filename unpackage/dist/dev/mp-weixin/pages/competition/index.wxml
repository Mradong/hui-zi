<view class="competition"><nav-bar vue-id="1" fontColor="#000" backState="1000" home="{{false}}" titleCenter="{{true}}" type="fixed" title="参与投标" bind:__l="__l"></nav-bar><view class="form-row"><label class="form-row-title">金额</label><input class="form-row-input " type="number" maxlength="11" placeholder="请输入投标金额" data-event-opts="{{[['input',[['__set_model',['','money','$event',[]]]]]]}}" value="{{money}}" bindinput="__e"/><block wx:if="{{money==''?true:false}}"><view class="warn"></view></block></view><button class="competition-button dominant-hue-button-bg-red" type="primary" data-event-opts="{{[['tap',[['openCompetition',['$event']]]]]}}" bindtap="__e">确认</button><uni-popup class="vue-ref" vue-id="2" type="dialog" data-ref="Competition" bind:__l="__l" vue-slots="{{['default']}}"><uni-popup-dialog vue-id="{{('3')+','+('2')}}" type="input" title="提示" content="{{Competition}}" duration="{{2000}}" before-close="{{true}}" data-event-opts="{{[['^close',[['close']]],['^confirm',[['confirm']]]]}}" bind:close="__e" bind:confirm="__e" bind:__l="__l"></uni-popup-dialog></uni-popup><uni-popup class="vue-ref" vue-id="4" type="message" data-ref="popup" bind:__l="__l" vue-slots="{{['default']}}"><uni-popup-message vue-id="{{('5')+','+('4')}}" type="{{type}}" message="{{message}}" duration="{{2000}}" bind:__l="__l"></uni-popup-message></uni-popup></view>