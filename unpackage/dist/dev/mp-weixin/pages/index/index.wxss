.home {
  width: 750rpx;
  height: 1334rpx;
  overflow: hidden;
  position: relative;
}
.home-logo {
  position: absolute;
  width: 100%;
  height: 100%;
}
.home-button {
  position: absolute;
  bottom: 40rpx;
  left: 50%;
  -webkit-transform: translate(-50%);
          transform: translate(-50%);
  width: 700rpx;
  border-radius: 20px;
  overflow: hidden;
}
.home-button button {
  color: #FFFCFD;
  background: -webkit-linear-gradient(bottom left, #FF6896, #FF5053);
  background: linear-gradient(to top right, #FF6896, #FF5053);
}

